<?xml version="1.0" encoding="UTF-8"?>
<web-app version="3.0" xmlns="http://java.sun.com/xml/ns/javaee" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-app_3_0.xsd">
    <servlet>
        <servlet-name>login</servlet-name>
        <servlet-class>login</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>index</servlet-name>
        <servlet-class>index</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>logout</servlet-name>
        <servlet-class>logout</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>retrieve_ticket</servlet-name>
        <servlet-class>retrieve_ticket</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>masalah_helper</servlet-name>
        <servlet-class>helper_masalah</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>ticket_helper</servlet-name>
        <servlet-class>helper_ticket</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>helper_atm</servlet-name>
        <servlet-class>helper_atm</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>retrieve_ticket</servlet-name>
        <url-pattern>/retrieve_ticket</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>index</servlet-name>
        <url-pattern>/index</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>logout</servlet-name>
        <url-pattern>/logout</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>login</servlet-name>
        <url-pattern>/login</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>masalah_helper</servlet-name>
        <url-pattern>/masalah_helper</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>ticket_helper</servlet-name>
        <url-pattern>/ticket_helper</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>helper_atm</servlet-name>
        <url-pattern>/helper_atm</url-pattern>
    </servlet-mapping>
    <session-config>
        <session-timeout>
            60
        </session-timeout>
    </session-config>
</web-app>
